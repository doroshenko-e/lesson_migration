<table class="table" >
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Birthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="sortable">
    <% @users.each do |user| %>
      <tr class="ui-state-default">
        <td><%= user.first_name %></td>
        <td><%= user.email %></td>
        <td><%= user.birthday %></td>
        <td><%= link_to 'Show', user %></td>
  
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @users %>